{
  "name": "@electric-examples/yjs-provider",
  "private": true,
  "version": "0.0.1",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "backend:up": "PROJECT_NAME=yjs-provider pnpm -C ../../ run example-backend:up && pnpm db:migrate",
    "backend:down": "PROJECT_NAME=yjs-provider pnpm -C ../../ run example-backend:down",
    "db:migrate": "dotenv -e ../../.env.dev -- pnpm exec pg-migrations apply --directory ./db/migrations",
    "dev": "next dev --turbo -p 5173",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "stylecheck": "eslint . --quiet",
    "typecheck": "tsc --noEmit",
    "test:browser": "playwright test ./e2e/"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.4",
    "@electric-sql/client": "workspace:*",
    "@electric-sql/react": "workspace:*",
    "@neondatabase/serverless": "^0.10.4",
    "codemirror": "^6.0.1",
    "lib0": "^0.2.96",
    "next": "^15.2.1",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sst": "3.6.35",
    "y-codemirror.next": "0.3.5",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "1.0.6",
    "yjs": "^13.6.24"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.3",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@next/eslint-plugin-next": "^15.2.1",
    "@playwright/test": "^1.51.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "globals": "^15.13.0",
    "playwright": "^1.51.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  }
}
